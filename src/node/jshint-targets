# jshint targets

jshint:
	@node node_modules/.bin/jshint $(HINT_DIR)
	@echo "Hinting Finished"

jshint-version:
	@node node_modules/.bin/jshint --version

jshint-help:
<%>src/assets/help-headline</%>
<%>src/node/jshint-help</%>
<%>src/assets/report-bugs</%>

.PHONY: jshint jshint-version jshint-help
